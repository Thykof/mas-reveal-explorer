{
  "manifest_version": 2,
  "name": "MasRevealExplorer",
  "version": "1.0",

  "description": "Reveal MAS addresses in the explore to easy-to-read name.",

  "icons": {
    "48": "icons/MasRevealExplorer-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.explorer.massa.net/*"],
      "js": ["MasRevealExplorer.js"]
    }
  ]
}
